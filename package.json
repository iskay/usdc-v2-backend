{
  "name": "usdc-v2-backend",
  "version": "0.1.0",
  "description": "Modular backend for blockchain transaction tracking and Cosmos forwarding",
  "type": "module",
  "main": "./dist/app.js",
  "exports": {
    ".": "./dist/app.js"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsup src/app.ts --format esm --dts --out-dir dist",
    "start": "node dist/app.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "format": "prettier --write \"src/**/*.{ts}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.2",
    "@prisma/client": "^6.19.0",
    "@types/ws": "^8.18.1",
    "awilix": "^12.0.5",
    "axios": "^1.13.1",
    "axios-retry": "^4.5.0",
    "bullmq": "^5.63.0",
    "date-fns": "^4.1.0",
    "envsafe": "^2.0.3",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.2",
    "pino": "^10.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.7",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
